<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.0.33</Version>
    <Authors>Atomiv</Authors>
    <Description>Implementation for Web Automation using Selenium</Description>
    <Copyright>?  All rights reserved.</Copyright>
    <PackageProjectUrl>https://atomiv.com</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryUrl>https://github.com/atomiv/atomiv-cs</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Atomiv Framework DotNetCore Infrastructure WebAutomation Selenium</PackageTags>
    <PackageIcon>favicon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetSeleniumExtras.PageObjects.Core" Version="3.12.0" />
    <PackageReference Include="DotNetSeleniumExtras.PageObjects.Core.Linq" Version="1.0.0" />
    <PackageReference Include="Selenium.WebDriver" Version="3.141.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Core\All\Atomiv.Core.All.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\img\favicon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
